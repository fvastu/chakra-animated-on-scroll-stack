{
  "name": "@fvastu/animated-on-scroll-stack",
  "version": "1.0.2",
  "description": "The easiest way to animated on scroll your chakraui/react project",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fvastu/chakra-animated-stack"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "nextjs",
    "Next.js",
    "chakraui/react",
    "chakraui animation",
    "scrollreveal",
    "Scrollreveal.js",
    "next.js animation",
    "animation",
    "transform",
    "transition",
    "scroll"
  ],
  "author": "Francesco Vasturzo",
  "license": "GPL-3.0",
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.2",
    "@types/node": "18.7.16",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.3",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "next": "12.3.0",
    "next-reveal": "^1.0.4",
    "react": ">=16",
    "react-dom": "18.2.0",
    "scrollreveal": "^4.0.9"
  }
}
